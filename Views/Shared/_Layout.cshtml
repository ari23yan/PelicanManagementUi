<!DOCTYPE html>
<html dir="rtl" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <meta name="robots" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Boltz : Crypto Admin Template">
    <meta property="og:title" content="Boltz : Crypto Admin Template">
    <meta property="og:description" content="Boltz : Crypto Admin Template">
    <meta property="og:image" content="https://boltz.dexignzone.com/xhtml/social-image.png">
    <meta name="format-detection" content="telephone=no">
    <title>داشبور مدیریت پلیکان</title>
    <link rel="shortcut icon" type="image/png" href="images/favicon.png">
    <link href="./vendor/owl-carousel/owl.carousel.css" rel="stylesheet">
    <link href="./vendor/jquery-nice-select/css/nice-select.css" rel="stylesheet">
    <link href="./css/style.css" rel="stylesheet">
</head>
<body>
    <div id="preloader">
        <div class="loader">
            <img src="~/images/loader.svg" />
        </div>
    </div>
    <div id="main-wrapper">
        <div class="nav-header">
            <a href="index.html" class="brand-logo">
                <img width="120" src="~/images/milad_hospital_logo.png" />
            </a><div class="nav-control">
                <div class="hamburger">
                    <span class="line"></span><span class="line"></span><span class="line"></span>
                </div>
            </div>
        </div>



        @* Header *@
        @Html.Partial("PartialViews/Header/_Header")

        @* SideBar *@

        @await Component.InvokeAsync("SideBar")

        @* MainContent *@
        <div class="content-body">
            @RenderBody()
        </div>




        @* footer *@
        @Html.Partial("PartialViews/Footer/_Footer")





    </div>
    <script src="./vendor/global/global.min.js"></script>
    <script src="./vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="vendor/jquery-nice-select/js/jquery.nice-select.min.js"></script>
    <script src="./vendor/apexchart/apexchart.js"></script>
    <script src="./vendor/owl-carousel/owl.carousel.js"></script>
    <script src="./js/dashboard/dashboard-1.js"></script>
    <script src="./js/custom.min.js"></script>
    <script src="./js/deznav-init.js"></script>

</body>
</html>












